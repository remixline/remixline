        let cart = [];
        const mainPage = document.getElementById('main-page');
        const cartPage = document.getElementById('cart-page');
        const viewCartBtn = document.getElementById('view-cart-btn');
        const backToShopBtn = document.getElementById('back-to-shop-btn');
        const productList = document.getElementById('product-list');
        const cartItemsContainer = document.getElementById('cart-items-container');
        const cartCount = document.getElementById('cart-count');
        const totalPriceEl = document.getElementById('total-price');
        const emptyCartMessage = document.getElementById('empty-cart-message');
        const checkoutProcess = document.getElementById('checkout-process');
        const saveInfoBtn = document.getElementById('save-info-btn');
        const infoSavedMessage = document.getElementById('info-saved-message');
        const fullNameInput = document.getElementById('full_name');
        const emailInput = document.getElementById('email');
        const notification = document.getElementById('add-to-cart-notification');
        let notificationTimeout;

        function showNotification() {
            if (notificationTimeout) {
                clearTimeout(notificationTimeout);
            }
            notification.classList.remove('opacity-0');
            notificationTimeout = setTimeout(() => {
                notification.classList.add('opacity-0');
            }, 3000);
        }

        function showCartPage() { mainPage.classList.add('hidden'); cartPage.classList.remove('hidden'); updateCartUI(); }
        function showMainPage() { cartPage.classList.add('hidden'); mainPage.classList.remove('hidden'); }

        viewCartBtn.addEventListener('click', showCartPage);
        backToShopBtn.addEventListener('click', showMainPage);

        saveInfoBtn.addEventListener('click', () => {
            const name = fullNameInput.value.trim();
            const email = emailInput.value.trim();

            if (name && email && /^\S+@\S+\.\S+$/.test(email)) {
                infoSavedMessage.classList.remove('hidden');
                setTimeout(() => { infoSavedMessage.classList.add('hidden'); }, 4000);
            }
        });

        function renderProducts() {
            productList.innerHTML = '';
            products.forEach(product => {
                const productCard = `<div class="glass-card p-6 rounded-2xl flex flex-col text-center items-center"><h3 class="text-xl font-bold text-white mb-2">${product.name}</h3><p class="text-2xl font-semibold text-blue-400 mb-4">$${product.price}</p><button class="add-to-cart-btn mt-auto bg-blue-600 text-white px-6 py-2 rounded-full hover:bg-blue-700 transition-colors w-full" data-id="${product.id}" data-key="add_to_cart">Add to Cart</button></div>`;
                productList.innerHTML += productCard;
            });
            addEventListenersToCartButtons();
            changeLanguage(document.documentElement.lang || 'en');
        }
        
        function addEventListenersToCartButtons() {
             document.querySelectorAll('.add-to-cart-btn').forEach(button => button.addEventListener('click', (e) => addToCart(parseInt(e.target.dataset.id))));
        }

        function addToCart(productId) {
            const product = products.find(p => p.id === productId);
            if(product) {
                cart.push(product);
                updateCartUI();
                viewCartBtn.classList.add('cart-btn-pop');
                setTimeout(() => viewCartBtn.classList.remove('cart-btn-pop'), 300);
                showNotification();
            }
        }

        function removeFromCart(index) { cart.splice(index, 1); updateCartUI(); }

        function updateCartUI() {
            cartItemsContainer.innerHTML = '';
            emptyCartMessage.classList.toggle('hidden', cart.length > 0);
            checkoutProcess.classList.toggle('hidden', cart.length === 0);
            
            if (cart.length > 0) {
                cart.forEach((item, index) => {
                    const cartItem = `<div class="glass-card p-4 rounded-lg flex items-center justify-between"><div><h4 class="font-bold text-white">${item.name}</h4><p class="text-sm text-gray-400">$${item.price}</p></div><button class="remove-from-cart-btn text-red-400 hover:text-red-300 text-sm" data-index="${index}" data-key="remove_item">Remove</button></div>`;
                    cartItemsContainer.innerHTML += cartItem;
                });
            }

            totalPriceEl.textContent = `$${cart.reduce((sum, item) => sum + item.price, 0)}`;
            cartCount.textContent = cart.length;

            document.querySelectorAll('.remove-from-cart-btn').forEach(button => button.addEventListener('click', e => removeFromCart(parseInt(e.target.dataset.index))));
            changeLanguage(document.documentElement.lang || 'en');
        }

        function changeLanguage(lang) {
            document.documentElement.lang = lang;
            document.documentElement.dir = (lang === 'fa' || lang === 'ar') ? 'rtl' : 'ltr';
            document.querySelectorAll('[data-key]').forEach(el => {
                const key = el.dataset.key;
                const translation = translations[lang]?.[key] || translations['en'][key];
                if (translation) {
                    if (key.includes('tx_info_instruction') || key.includes('code_usage_notice')) el.innerHTML = translation;
                    else el.textContent = translation;
                }
            });
            document.querySelectorAll('.lang-btn').forEach(btn => btn.classList.toggle('active', btn.dataset.lang === lang));
        }
        
        document.querySelectorAll('.lang-btn').forEach(button => button.addEventListener('click', (e) => changeLanguage(e.target.dataset.lang)));

        document.querySelectorAll('.payment-tab').forEach(tab => {
            tab.addEventListener('click', () => {
                document.querySelectorAll('.payment-tab').forEach(t => t.classList.remove('tab-active'));
                document.querySelectorAll('.payment-content').forEach(c => c.classList.add('hidden'));
                tab.classList.add('tab-active');
                document.getElementById(tab.dataset.target + '-content').classList.remove('hidden');
            });
        });
        
        function showCopiedMessage(button) {
            const lang = document.documentElement.lang || 'en';
            const originalText = translations[lang]?.['copy_address_btn'] || 'Copy Address';
            const copiedText = translations[lang]?.['copied_text'] || 'Copied!';
            button.textContent = copiedText;
            setTimeout(() => {
                button.textContent = originalText;
            }, 2000);
        }

        function copyToClipboard(text, button) {
            if (navigator.clipboard && window.isSecureContext) {
                navigator.clipboard.writeText(text).then(() => {
                    showCopiedMessage(button);
                }).catch(() => {
                    fallbackCopy(text, button);
                });
            } else {
                fallbackCopy(text, button);
            }
        }

        function fallbackCopy(text, button) {
            const textArea = document.createElement('textarea');
            textArea.value = text;
            textArea.style.position = 'fixed';
            textArea.style.top = '-9999px';
            textArea.style.left = '-9999px';
            document.body.appendChild(textArea);
            textArea.focus();
            textArea.select();
            try {
                if (document.execCommand('copy')) {
                    showCopiedMessage(button);
                }
            } catch (err) {
                 console.error('Fallback copy failed', err);
            }
            document.body.removeChild(textArea);
        }

        document.querySelectorAll('.copy-btn').forEach(button => {
            button.addEventListener('click', () => {
                const addressText = document.getElementById(button.dataset.address).innerText;
                copyToClipboard(addressText, button);
            });
        });


        const observer = new IntersectionObserver(entries => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.classList.add('is-visible');
                    observer.unobserve(entry.target);
                }
            });
        }, { threshold: 0.1 });
        document.querySelectorAll('.fade-in-section').forEach(section => observer.observe(section));
        
        renderProducts();
        updateCartUI();
        changeLanguage('en');
    });
    </script>
