        }

        .fade-in-section.is-visible {
            opacity: 1;
            transform: translateY(0);
        }

        .cart-btn-pop {
            animation: pop 0.3s ease-out;
        }
        @keyframes pop {
            0% { transform: scale(1); }
            50% { transform: scale(1.2); }
            100% { transform: scale(1); }
        }
